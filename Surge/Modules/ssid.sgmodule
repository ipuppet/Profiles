#!name=SSID Switcher
#!desc=根据 SSID 自动更改策略组和 Outbound Mode
#!arguments=override:"false",notification:"None",default:"",ssidConfig:""
#!arguments-desc=override:[false|true]

[Script]
ssid = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/ipuppet/Profiles/master/Scripts/flush-dns.js,argument=override={{{override}}}notification={{{notification}}}&default={{{default}}}&ssidConfig={{{ssidConfig}}}
